{
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "node": true
  },
  "plugins": [
      "react",
      "react-native"
  ],
    "ecmaFeatures": {
    "jsx": true
  },
  "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended"
  ],
  "rules": {
    "react-native/no-unused-styles": 2,
    "react-native/split-platform-components": 2,
  }
}
